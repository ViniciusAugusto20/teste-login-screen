{
  "name": "my-store",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.5",
    "@types/react": "^16.9.55",
    "@types/uuid": "^8.3.4",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "cypress": "^8.1.0",
    "formik": "^2.2.6",
    "global": "^4.4.0",
    "i": "^0.3.6",
    "immer": "^8.0.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.13.1",
    "react-dom": "16",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "styled-components": "^5.2.0",
    "styled-media-query": "^2.1.2",
    "typescript": "^4.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.{ts,js,tsx,jsx}\" -f table",
    "lint:fix": "eslint ./src --ext .js,.ts,.tsx --fix",
    "prettier:check": "npx prettier --list-different \"src/**/*.{ts,js,tsx,jsx}\"",
    "prettier:fix": "npx prettier --write \"src/**/*.{ts,js,tsx,jsx}\"",
    "lint:css": "stylelint \"./src/**/style.ts\" --color -f verbose"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint --fix",
      "yarn prettier:fix"
    ],
    "*style.ts": [
      "stylelint --color -f verbose"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "react-app-env.d.ts",
    "serviceWorker.ts",
    "setupTests.ts",
    "commitlint.config.js"
  ],
  "extends": [
    "plugin:cypress/recommended"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 9",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.25",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "react-error-overlay": "6.0.9",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "4.42.0"
  }
}
